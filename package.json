{
  "name": "CICD-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run type-check-watch & npm run serve",
    "type-check": "tsc --noEmit",
    "type-check-watch": "tsc --noEmit --watch",
    "build": "webpack --config ./config/webpack.common.js --env production",
    "serve": "webpack serve --config ./config/webpack.common.js --env development",
    "lint": "eslint --ext .js,.ts --quiet src --fix && npx lint-staged",
    "test": "jest"
  },
  "pre-commit": [
    "lint"
  ],
  "lint-staged": {
    "*.{css,scss,html,md}": [
      "prettier --write"
    ],
    "*.{js,ts}": [
      "jest --bail --passWithNoTests --findRelatedTests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guojingwen/CICD-demo.git"
  },
  "keywords": [],
  "author": "dashu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guojingwen/CICD-demo/issues"
  },
  "homepage": "https://github.com/guojingwen/CICD-demo#readme",
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^5.5.3",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.29",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.3",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "core-js": "^3.32.2",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^29.7.0"
  }
}